{
  "files": [],
  "references": [{ "path": "./tsconfig.app.json" }, { "path": "./tsconfig.node.json" }],
  "compilerOptions": {
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "jsx": "react",
    "target": "ES6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "client/public/src",
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types", "client/public/src/types"]
  },
  "include": ["client/public/src/**/*"]

}

